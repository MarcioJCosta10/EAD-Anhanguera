algoritmo "Viagem Disney"

var
   Ano, nasc, idade: Inteiro

inicio
      Escreva("Em que ano estamos? ")
      Leia (Ano)
      Escreva("Em que ano você nasceu? ")
      Leia(nasc)
      idade <- ano-nasc
      Escreva ("Em ", ano, " voce terá ", idade ," anos ")
              Se (idade>=21) entao
                 Escreval(" e já terá atingido a maioridade.")
              Fimse


fimalgoritmo
